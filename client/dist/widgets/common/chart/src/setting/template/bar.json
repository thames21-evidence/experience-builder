{
  "version": "24.0.0",
  "id": "bar",
  "type": "chart",
  "dataSource": {
    "query": {
      "groupByFieldsForStatistics": [],
      "outStatistics": []
    }
  },
  "rotated": true,
  "colorMatch": true,
  "stackedType": "sideBySide",
  "series": [
    {
      "type": "barSeries",
      "dataTooltipVisible": true,
      "binTemporalData": false,
      "dataLabels": {
        "type": "chartText",
        "visible": false,
        "content": {
          "type": "esriTS",
          "color": "var(--sys-color-surface-paper-hint)",
          "font": {
            "family": "inherit",
            "size": 10,
            "style": "normal",
            "weight": "normal",
            "decoration": "none"
          },
          "horizontalAlignment": "right",
          "verticalAlignment": "middle"
        }
      },
      "fillSymbol": {
        "type": "esriSFS",
        "style": "esriSFSSolid",
        "color": "#5E8FD0",
        "outline": {
          "type": "esriSLS",
          "style": "esriSLSSolid",
          "color": "var(--sys-color-divider-secondary)",
          "width": 0
        }
      },
      "hideOversizedStackedLabels": false
    }
  ],
  "axes": [
    {
      "type": "chartAxis",
      "visible": true,
      "isLogarithmic": false,
      "title": {
        "type": "chartText",
        "visible": false,
        "content": {
          "type": "esriTS",
          "color": "var(--sys-color-surface-paper-text)",
          "font": {
            "family": "inherit",
            "size": 14,
            "style": "normal",
            "weight": "normal",
            "decoration": "none"
          },
          "horizontalAlignment": "center",
          "verticalAlignment": "middle"
        }
      },
      "labels": {
        "type": "chartText",
        "visible": true,
        "content": {
          "type": "esriTS",
          "color": "var(--sys-color-surface-paper-hint)",
          "font": {
            "family": "inherit",
            "size": 12,
            "style": "normal",
            "weight": "normal",
            "decoration": "none"
          },
          "horizontalAlignment": "center",
          "verticalAlignment": "middle"
        }
      },
      "valueFormat": {
        "type": "category",
        "characterLimit": 11
      },
      "lineSymbol": {
        "type": "esriSLS",
        "style": "esriSLSSolid",
        "color": "var(--sys-color-divider-primary)",
        "width": 1
      }
    },
    {
      "type": "chartAxis",
      "visible": true,
      "isLogarithmic": false,
      "title": {
        "type": "chartText",
        "visible": false,
        "content": {
          "type": "esriTS",
          "color": "var(--sys-color-surface-paper-text)",
          "font": {
            "family": "inherit",
            "size": 14,
            "style": "normal",
            "weight": "normal",
            "decoration": "none"
          },
          "horizontalAlignment": "center",
          "verticalAlignment": "middle"
        }
      },
      "labels": {
        "type": "chartText",
        "visible": true,
        "content": {
          "type": "esriTS",
          "color": "var(--sys-color-surface-paper-hint)",
          "font": {
            "family": "inherit",
            "size": 12,
            "style": "normal",
            "weight": "normal",
            "decoration": "none"
          },
          "horizontalAlignment": "center",
          "verticalAlignment": "middle"
        }
      },
      "valueFormat": {
        "type": "number",
        "intlOptions": {
          "style": "decimal",
          "useGrouping": true,
          "minimumFractionDigits": 0,
          "maximumFractionDigits": 0
        }
      },
      "lineSymbol": {
        "type": "esriSLS",
        "style": "esriSLSSolid",
        "color": "var(--sys-color-divider-primary)",
        "width": 1
      }
    }
  ]
}