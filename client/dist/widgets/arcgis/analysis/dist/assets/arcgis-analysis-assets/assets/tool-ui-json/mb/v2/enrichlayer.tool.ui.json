{
  "toolName": "EnrichLayer",
  "version": "0.1.0",
  "modelBuilderVersion": "V2",
  "rules": [
    {
      "ruleType": "expression",
      "expression": "$[Component].inputLayer.selectedLayers.geometryType = 'polygon'",
      "destination": ["distance", "returnBoundaries", "bufferType"],
      "effectParams": {
        "visible": false
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].inputLayer.selectedLayers.geometryType NOT null) AND ($[Component].inputLayer.selectedLayers.geometryType NOT 'polygon'))",
      "destination": ["distance"],
      "effectParams": {
        "required": true
      }
    },
    {
      "ruleType": "expression",
      "expression": "($[Component].bufferType.choiceList.length = 1) AND ($[Data].bufferType = 'StraightLine'))",
      "destination": "bufferType",
      "effectParams": {
        "readOnly": true
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].inputLayer.selectedLayers.geometryType = 'point') OR ($[Component].inputLayer.selectedLayers.geometryType = 'multipoint'))",
      "destination": "bufferType",
      "effectParams": {
        "value": "StraightLine",
        "readOnly": true
      }
    },
    {
      "ruleType": "expression",
      "expression": "$[Component].inputLayer.selectedLayers.geometryType = 'polyline'))",
      "destination": "bufferType",
      "effectParams": {
        "value": "StraightLine",
        "readOnly": true
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].inputLayer.selectedLayers.geometryType = null) OR ($[Component].inputLayer.selectedLayers.geometryType = 'polygon'))",
      "destination": "bufferType",
      "effectParams": {
        "value": "StraightLine",
        "readOnly": true
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Data].distance.value.units = 'Miles') AND ($[Data].bufferType = 'StraightLine'))",
      "destination": "distance",
      "effectParams": {
        "maximum": 1000
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Data].distance.value.units = 'Kilometers') AND ($[Data].bufferType= 'StraightLine'))",
      "destination": "distance",
      "effectParams": {
        "maximum": 1609
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Minutes') AND ($[Data].bufferType.impedanceAttributeName NOT 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 300
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Hours') AND ($[Data].bufferType.impedanceAttributeName NOT 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 5
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Seconds') AND ($[Data].bufferType.impedanceAttributeName NOT 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 18000
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Minutes') AND ($[Data].bufferType.impedanceAttributeName = 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 540
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Hours') AND ($[Data].bufferType.impedanceAttributeName = 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 9
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Seconds') AND ($[Data].bufferType.impedanceAttributeName = 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 32400
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Miles') AND ($[Data].bufferType.timeAttributeName NOT 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 300
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Miles') AND ($[Data].bufferType.timeAttributeName = 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 27
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Kilometers') AND ($[Data].bufferType.timeAttributeName NOT 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 482.803
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Kilometers') AND ($[Data].bufferType.timeAttributeName = 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 43.452
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Meters') AND ($[Data].bufferType.timeAttributeName NOT 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 482803.2
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Meters') AND ($[Data].bufferType.timeAttributeName = 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 43452.288
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Feet') AND ($[Data].bufferType.timeAttributeName NOT 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 1584000
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Feet') AND ($[Data].bufferType.timeAttributeName = 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 142560
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Yards') AND ($[Data].bufferType.timeAttributeName NOT 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 528000
      }
    },
    {
      "ruleType": "expression",
      "expression": "(($[Component].currentTool.portal.isPortal = false) AND ((($[Data].distance.value.units = 'Yards') AND ($[Data].bufferType.timeAttributeName = 'WalkTime'))))",
      "destination": "distance",
      "effectParams": {
        "maximum": 47520
      }
    },
    {
      "ruleType": "expression",
      "expression": "($[Data].bufferType = 'StraightLine')",
      "destination": "distance",
      "effectParams": {
        "maximum": null
      }
    }
  ],
  "UIparameters": [
    {
      "label": "$featuresToEnrichSectionLabel",
      "componentName": "analysis-block",
      "summaryText": "$featuresToEnrichSectionText",
      "UIparameters": [
        {
          "name": "inputLayer",
          "label": "$inputFeatures",
          "componentName": "analysis-layer-input",
          "layerFilterType": ["feature"],
          "enableSketch": true
        }
      ]
    },
    {
      "label": "$enrichmentVariablesSectionLabel",
      "componentName": "analysis-block",
      "summaryText": "$enrichmentVariablesSectionText",
      "UIparameters": [
        {
          "name": "enrichVariables",
          "helpId": "analysisVariables",
          "label": "$enrichmentVariables",
          "componentName": "analysis-geoenrichment-input",
          "required": true
        }
      ]
    },
    {
      "name": "enrichmentAreas",
      "label": "$enrichmentAreasSectionLabel",
      "componentName": "analysis-block",
      "summaryText": "$enrichmentAreasSectionText",
      "UIparameters": [
        {
          "name": "bufferType",
          "label": "$measurementTypeLabel",
          "required": true,
          "defaultValue": "StraightLine",
          "value": "StraightLine",
          "readOnly": true
        },
        {
          "name": "distance",
          "label": "$measurementDistanceLabel",
          "UIparameterInfos": [
            {
              "name": "linearUnit",
              "dataType": "GPLinearUnit",
              "unitChoiceList": ["Meters", "Kilometers", "Miles", "Feet", "Yards"],
              "choiceListLabels": {
                "Meters": "$meters",
                "Kilometers": "$kilometers",
                "Miles": "$miles",
                "Feet": "$feet",
                "Yards": "$yards"
              },
              "minimum": 0,
              "excludeMinValue": true
            }
          ]
        },
        {
          "name": "returnBoundaries",
          "label": "$returnBoundariesLabel"
        }
      ]
    },
    {
      "label": "$resultLayerGroupLabel",
      "layout": "vertical",
      "componentName": "analysis-block",
      "summaryText": "$outputSectionText",
      "UIparameters": [
        {
          "name": "outputName",
          "label": "$outputName",
          "placeholderText": "$outputNamePlaceholderText",
          "required": true
        },
        {
          "name": "saveResultIn",
          "label": "$saveResultIn",
          "componentName": "analysis-folder-input"
        }
      ]
    }
  ],
  "outputParameters": [
    {
      "label": "$enrichedLayerOutput",
      "name": "enrichedLayer"
    }
  ]
}
