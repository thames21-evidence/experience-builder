{
  "toolName": "GenerateTessellations",
  "version": "0.1.0",
  "modelBuilderVersion": "V2",
  "rules": [
    {
      "ruleType": "expression",
      "destination": "binResolution",
      "expression": "$[Data].binType = 'H3_HEXAGON'",
      "effectParams": {
        "visible": true,
        "required": true
      }
    },
    {
      "ruleType": "expression",
      "destination": "binSize",
      "expression": "$[Data].binType = 'H3_HEXAGON'",
      "effectParams": {
        "visible": false,
        "required": false
      }
    },
    {
      "ruleType": "expression",
      "destination": "binResolution",
      "expression": "$[Data].binType NOT 'H3_HEXAGON'",
      "effectParams": {
        "visible": false,
        "required": false
      }
    }
  ],
  "UIparameters": [
    {
      "label": "$tessellationSettingsLabel",
      "componentName": "analysis-block",
      "summaryText": "$tessellationSettingsSummaryText",
      "UIparameters": [
        {
          "name": "binType",
          "label": "$binType",
          "defaultValue": "SQUARE",
          "choiceListLabels": {
            "HEXAGON": "$hexagonBins",
            "SQUARE": "$squareBins",
            "TRIANGLE": "$triangleBins",
            "DIAMOND": "$diamondBins",
            "TRANSVERSEHEXAGON": "$thexagonBins",
            "H3_HEXAGON": "$h3hexagonBins"
          },
          "choiceListIcons": {
            "HEXAGON": "grid-hexagon",
            "SQUARE": "grid-unit",
            "TRIANGLE": "grid-triangle",
            "DIAMOND": "grid-diamond",
            "TRANSVERSEHEXAGON": "grid-hexagon",
            "H3_HEXAGON": "h3-hexagon"
          }
        },
        {
          "name": "binResolution",
          "label": "$binResolution",
          "componentName": "analysis-number-input",
          "required": false,
          "visible": false,
          "min": 0,
          "max": 15
        },
        {
          "UIparameters": [
            {
              "name": "binSize",
              "label": "$binSizeLabel",
              "required": true,
              "UIparameterInfos": [
                {
                  "name": "linearUnit",
                  "dataType": "GPLinearUnit",
                  "unitChoiceList": ["Kilometers", "Meters", "Miles", "NauticalMiles", "Feet", "Yards"],
                  "choiceListLabels": {
                    "Kilometers": "$kilometers",
                    "Meters": "$meters",
                    "Miles": "$miles",
                    "NauticalMiles": "$nauticalMiles",
                    "Feet": "$feet",
                    "Yards": "$yards"
                  },
                  "minimum": 0,
                  "excludeMinValue": true
                },
                {
                  "name": "arealUnit",
                  "dataType": "GPArealUnit",
                  "unitChoiceList": [
                    "SquareKilometers",
                    "SquareMeters",
                    "Hectares",
                    "SquareMiles",
                    "SquareYards",
                    "SquareFeet",
                    "SquareInches",
                    "Acres"
                  ],
                  "choiceListLabels": {
                    "SquareMeters": "$squareMeters",
                    "SquareKilometers": "$squareKilometers",
                    "Hectares": "$hectares",
                    "SquareFeet": "$squareFeet",
                    "SquareYards": "$squareYards",
                    "SquareMiles": "$squareMiles",
                    "SquareInches": "$squareInches",
                    "Acres": "$acres"
                  },
                  "minimum": 0,
                  "excludeMinValue": true
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "label": "$extentSectionLabel",
      "componentName": "analysis-block",
      "summaryText": "$extentSectionText",
      "UIparameters": [
        {
          "name": "extentLayer",
          "label": "$extentLayer",
          "componentName": "analysis-layer-input",
          "layerFilterType": ["feature"],
          "enableSketch": true,
          "required": true
        },
        {
          "name": "intersectStudyArea",
          "label": "$intersectStudyArea"
        }
      ]
    },
    {
      "label": "$resultLayerGroupLabel",
      "componentName": "analysis-block",
      "summaryText": "$outputAreasSectionText",
      "UIparameters": [
        {
          "name": "outputName",
          "label": "$outputName",
          "componentName": "analysis-string-input",
          "placeholderText": "$outputNamePlaceholderText",
          "required": true
        },
        {
          "label": "$saveResultIn",
          "componentName": "analysis-folder-input",
          "name": "saveResultIn"
        }
      ]
    }
  ],
  "validationRules": [],
  "outputParameters": [
    {
      "label": "$tessellationLayerOutput",
      "name": "tessellationLayer"
    }
  ]
}
